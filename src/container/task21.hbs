<div>
  <section>
    {{#with heading}}
      <div class='card'>
        <div class='card-body'>
          <div class='card'>
            <div class='card-body'>
              <h1>Main:</h1>{{#with main}}
                <div class='text-muted'>
                  <p>{{title}}</p>
                  <p>{{text}}</p>
                  <div><button
                      type='button'
                      class='btn btn-primary'
                    >
                      {{button_text.solid}}</button>
                  </div>
                  <br />
                  <div><button
                      type='button'
                      class='btn btn-light'
                    >
                      {{button_text.outline}}</button>
                  </div>
                </div>
              {{/with}}
            </div>
          </div>
          <br />
          <div class='card'>
            <div class='card-body'>
              <h2>Second: </h2>{{#with second}}
                <div class='text-muted'>
                  <p>{{title}}</p>
                  <p>{{text}}</p>
                  <div><button
                      type='button'
                      class='btn btn-secondary'
                    >
                      {{button_text.solid}}</button>
                  </div>
                  <br />
                  <div><button
                      type='button'
                      class='btn btn-light'
                    >
                      {{button_text.outline}}</button>
                  </div>
                </div>
              {{/with}}
            </div>
          </div>
          <br />
          <div class='card'>
            <div class='card-body'>
              <h3>Subblock: </h3>{{#with subblock}}
                <div class='text-muted'>
                  <p>{{title}}</p>
                  <p>{{text}}</p>
                  <div><button
                      type='button'
                      class='btn btn-dark'
                    >
                      {{button_text.solid}}</button>
                  </div>
                  <br />
                  <div><button
                      type='button'
                      class='btn btn-light'
                    >
                      {{button_text.outline}}</button>
                  </div>
                </div>
              {{/with}}
            </div>
          </div>
        </div>
      </div>
    {{/with}}
  </section>
  <section>
    {{#with nested_display}}
      <div class='card'>
        <div class='card-body'>
          <p class='display-1'>{{title}}</p>
            <div class='card'>
            <div class='card board'>
                <section>
                    {{#with children_display}}
                    <p class='display-4'>children</p>
                    <p>{{title}}</p>
                    <p>{{text}}</p>
                    <div><button type='button' class='btn btn-outline-warning'>
                    {{button.text}}</button>
                    {{/with}}
                    </div>
                </section>
            </div>
            </div>
            <br>
            <div>
                <h6>{{header_text}}</h6>
                <br>
                <div><button type='button' class='btn btn-info'>
                {{button.text}}</button>
                </div>
            </div>
        </div>
      </div>
    {{/with}}
  </section>
  <section>
   {{#with paragraph}}
  <div class='card'>
    <div class='card-body'>
        <p class="text-uppercase">{{title}}</p>
        <p class="text-lowercase">{{text}}</p>
        <a href="https://google.com" class='btn btn-link'>
        {{button.text}}</a>
      <div class='card'>
        <div class='card-body'>
            <ul>{{#each list_name}}
                <li><a href="https://github.com/" class='btn btn-link'>
                {{text}}</a></li>
                {{/each}}
            </ul>
        </div>
      </div>
    </div>
  </div>
  {{/with}}
  </section>
</div>