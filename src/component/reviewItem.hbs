{{#each reviewList}}
<div class="card p-3 mb-5">
    <div class="card">
        <ul class="list-group">
            <li class="list-group-item">
                <div class="ms-3">
                    {{#with title}}
                    <p><b>{{userName}}</b> </p>
                    <p>{{caption}}</p>
                    <p>{{date}}</p>
                    {{/with}}
                </div>
            </li>
            <li class="list-group-item">
                <div class="card-body">
                    <div>{{#with reviewBody}}
                        <div>
                            {{#with seller}}
                            <p>{{title}}: {{value}}</p>
                            {{/with}}
                        </div>
                        <div class="mt-3">
                            {{#with rating}}
                            <p>{{title}}: {{value}}</p>
                            {{/with}}
                        </div>
                        <div class="mt-3">
                            {{#each text}}
                            {{this}}
                            {{/each}}
                        </div>
                        <div class="mt-3">
                            {{#with advantages}}
                            <b>{{title}}</b>
                            <p>{{text}}</p>
                            {{/with}}
                        </div>
                        <div class="mt-3">
                            {{#with disadvantages}}
                            <b>{{title}}</b>
                            <p>{{text}}</p>
                            {{/with}}
                        </div>
                        <div class="mt-3">
                            {{#each images}}
                            <img src='{{this}}' />
                            {{/each}}
                        </div>
                        {{/with}}
                    </div>
                </div>
            </li>
        </ul>
    </div>
    <div class="ms-5 ">
        {{#each reviewBody.coments}}
        <div class="card pmt-3 mb-3">
            <b>{{userName}}</b>
            <p>{{date}}</p>
            <p>{{text}}</p>
        </div>
        {{/each}}
    </div>
</div>
{{/each}}