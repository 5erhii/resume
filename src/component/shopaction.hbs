{{#with service}}
  <div class='card'>
    <div class='text-bg-success text-center'>
      <h2>{{title}}</h2>
      <p>{{description}}</p>
      {{#each buttons}}
        <a
          href='{{link}}'
          rel='{{rel}}'
          class='btn btn-primary mb-3'
        >{{text}}</a>
      {{/each}}
    </div>
  </div>
{{/with}}